    "def join_sentences_lcel(sentence1, sentence2):\n",
    "    return f\"{sentence1[:-1]}, and {sentence2.lower()}\"\n",
    "\n",
    "# Example usage\n",
    "sentence1 = \"Hello, World!\"\n",
    "sentence2 = \"How are you?\"\n",
    "lcel_output = join_sentences_lcel(sentence1, sentence2)\n",
    "print(lcel_output)\n"
   ]
